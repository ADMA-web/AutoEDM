		td[class=em_height]{height:20px!important;}
		td[class=em_height1]{height:10px!important;}
		td[class=em_align_cent]{text-align:center!important;}
		td[class=em_left]{text-align:left!important; font-size:24px!important; line-height:26px!important;}
		td[class=em_align_cent1]{text-align:center!important;font-size:11px!important;line-height:14px!important;}
		td[class=em_pad]{padding-left:10px!important;}
		td[class=em_side]{padding-left:10px!important;padding-right:10px!important;}
		div[class=hide_disktop]{display:table!important;float:none!important;width:100%!important;overflow:visible!important;height:auto!important;}
		table[class=em_tbl_312]{width:312px!important;}
		}
		@media only screen and (max-width:480px){
		table[class=em_wrapper]{width:100%!important;}
